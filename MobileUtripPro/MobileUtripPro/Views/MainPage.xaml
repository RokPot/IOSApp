            </NavigationPage.Icon>
            <x:Arguments>
                <views:RegistrationPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Checklist" BarBackgroundColor="#18b300">
            <NavigationPage.Icon>
                <OnPlatform x:TypeArguments="FileImageSource">
