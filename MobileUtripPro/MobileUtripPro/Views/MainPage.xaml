                </OnPlatform>
            </NavigationPage.Icon>
            <x:Arguments>
                <views:ChecklistPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="Stopppage" BarBackgroundColor="#18b300">
            <NavigationPage.Icon>
                <OnPlatform x:TypeArguments="FileImageSource">
                    <On Platform="iOS" Value="Stoppage_icon.png"/>
                </OnPlatform>
            </NavigationPage.Icon>
            <x:Arguments>
                <views:StoppagePage />
            </x:Arguments>
        </NavigationPage>
